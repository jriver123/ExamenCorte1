Proceso CalcularPromedio
	Definir nota1, nota2, nota3, nota4, nota5, promedio Como Real;
	Escribir "ingresa la nota1:";
	leer nota1;
	Escribir "ingresa la nota2:";
	Leer nota2;
	Escribir "ingresa la nota3:";
	Leer nota3;
	Escribir "ingresa la nota4:";
	leer nota4;
	Escribir "ingresa la nota5:";
	leer nota5;
	
	promedio <- (nota1 + nota2 + nota3 + nota4 + nota5) / 5;
	Escribir "el promedio es:" , promedio;
	si promedio > 85 entonces 
		Escribir "felicidades su promedio es mayor a 85.";
	SiNo
		Escribir "animos siue intentando.";
		
	FinSi
	
	
	
	
FinProceso
